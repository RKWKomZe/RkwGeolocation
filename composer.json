{
    "name": "rkw/rkw-geolocation",
    "type": "typo3-cms-extension",
    "description": "Locates addresses and zip codes with Google Geo-API and displays them in Google Maps",
    "homepage": "https://www.rkw-kompetenzzentrum.de",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "RKW", "Geolocation", "Google Maps"],
    "support": {
        "email": "developer@steffenkroggel.de"
    },
    "authors": [
        {
            "name": "Steffen Kroggel",
            "email": "developer@steffenkroggel.de",
            "homepage": "http://www.steffenkroggel.de",
            "role": "Developer"
        },
        {
            "name": "Maximilian Fäßler",
            "email": "maximilian@faesslerweb.de",
            "homepage": "https://www.faesslerweb.de",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^7.6.0 || ^8.7.0",
    },
    "autoload": {
        "psr-4": {
            "RKW\\Geolocation\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "rkw_geolocation"
        }
    }
}